{
  "name": "real-demo-app",
  "version": "1.0.1",
  "private": true,
  "description": "",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/ademkoc/real-demo-app.git"
  },
  "license": "MIT",
  "author": "Adem KoÃ§",
  "type": "module",
  "scripts": {
    "prebuild": "pnpm lint && rimraf ./dist",
    "build": "tsc",
    "check-updates": "pnpm outdated --recursive --long",
    "start": "node --experimental-strip-types --env-file=.env src/server.ts",
    "dev": "node --experimental-strip-types --watch --watch-preserve-output --env-file=.env src/server.ts",
    "lint": "eslint && jsonschema lint ./schemas",
    "generate-types": "node bin/generate-api-types.mjs",
    "lint:fix": "eslint --fix",
    "migration:create": "NODE_OPTIONS='--experimental-strip-types -r dotenv/config' mikro-orm migration:create",
    "migration:down": "NODE_OPTIONS='--experimental-strip-types -r dotenv/config' mikro-orm migration:down",
    "migration:up": "NODE_OPTIONS='--experimental-strip-types -r dotenv/config' mikro-orm migration:up",
    "release": "semantic-release",
    "test": "node --experimental-strip-types --test ./test/*.ts"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "breaking",
              "release": "major"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "chore",
                "section": "Chores"
              },
              {
                "type": "docs",
                "section": "Documentation"
              },
              {
                "type": "refactor",
                "section": "Refactors"
              },
              {
                "type": "test",
                "section": "Tests"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@adonisjs/health": "^2.0.0",
    "@fastify/otel": "^0.8.0",
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "@mikro-orm/core": "^6.4.15",
    "@mikro-orm/mysql": "^6.4.15",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.200.0",
    "@opentelemetry/instrumentation-http": "^0.200.0",
    "@opentelemetry/instrumentation-mysql2": "^0.46.0",
    "@opentelemetry/instrumentation-undici": "^0.11.0",
    "@opentelemetry/resources": "^2.0.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@opentelemetry/semantic-conventions": "^1.33.0",
    "fastify": "^5.3.2",
    "fastify-metrics": "^12.1.0",
    "fastify-plugin": "^5.0.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.4.15",
    "@mikro-orm/migrations": "^6.4.15",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@sourcemeta/jsonschema": "^9.2.4",
    "@types/node": "22.15.17",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "dotenv": "^16.5.0",
    "eslint": "9.26.0",
    "fastify-tsconfig": "3.0.0",
    "json-schema-to-typescript": "15.0.4",
    "neostandard": "0.12.1",
    "rimraf": "6.0.1",
    "semantic-release": "^24.2.3",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.6.2",
  "engines": {
    "node": ">=22"
  }
}
